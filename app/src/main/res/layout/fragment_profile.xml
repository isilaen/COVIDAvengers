<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/profileMain"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="50dp"
        android:paddingTop="32dp"
        android:paddingLeft="32dp"
        android:paddingRight="32dp"
        android:layout_gravity="center"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/profileMainRow1"
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:layout_marginBottom="18dp"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:background="@drawable/semi_transparent_button">

                <ImageView
                    android:id="@+id/profile_image"
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    android:layout_margin="20dp"
                    android:layout_gravity="center"
                    android:src="@drawable/blue_circle" />

                <LinearLayout
                    android:id="@+id/detailsRow1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:background="@drawable/semi_transparent_button">

                    <TextView
                        android:id="@+id/nameTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="30sp"
                        android:textFontWeight="700"
                        android:text="Patient Name"
                        android:layout_marginBottom="5dp"
                        android:textColor="@color/colorPrimaryDark"/>

                    <TextView
                        android:id="@+id/ageTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="22sp"
                        android:textFontWeight="700"
                        android:text="Age"
                        android:layout_marginBottom="5dp"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/profileMainRow2"
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:layout_marginBottom="18dp"
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:background="@drawable/semi_transparent_button">

            <LinearLayout
                android:id="@+id/weightInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="32dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/semi_transparent_button">

                <TextView
                    android:id="@+id/weightTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="12sp"
                    android:textFontWeight="700"
                    android:text="WEIGHT"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

                <TextView
                    android:id="@+id/weightValueTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="22sp"
                    android:textFontWeight="700"
                    android:text="70"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/glucoseInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/semi_transparent_button">

                <TextView
                    android:id="@+id/glucoseTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="12sp"
                    android:textFontWeight="700"
                    android:text="GLUCOSE"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

                <TextView
                    android:id="@+id/glucoseValueTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="22sp"
                    android:textFontWeight="700"
                    android:text="70"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/pressureInfo"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:orientation="vertical"
                android:layout_marginLeft="30dp"
                android:layout_marginRight="30dp"
                android:background="@drawable/semi_transparent_button">

                <TextView
                    android:id="@+id/pressureTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="12sp"
                    android:textFontWeight="700"
                    android:text="WEIGHT"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

                <TextView
                    android:id="@+id/pressureValueTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/fonts"
                    android:textSize="22sp"
                    android:textFontWeight="700"
                    android:text="70"
                    android:layout_gravity="center"
                    android:textColor="@color/colorPrimaryDark"/>

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/achievementsMain"
            android:layout_width="match_parent"
            android:layout_height="300dp"
            android:layout_gravity="center"
            android:orientation="vertical"
            android:background="@drawable/semi_transparent_button">

            <LinearLayout
                android:id="@+id/achievementsRow1"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/achievmentItem1"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement1Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement1TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:text="ACHIEVEMENT"
                        android:layout_marginTop="3dp"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/achievmentItem2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement2Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement2TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:layout_marginTop="3dp"
                        android:text="ACHIEVEMENT"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/achievmentItem3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement3Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement3TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:layout_marginTop="3dp"
                        android:text="ACHIEVEMENT"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:id="@+id/achievementsRow2"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:orientation="horizontal">

                <LinearLayout
                    android:id="@+id/achievmentItem4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement4Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement4TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:text="ACHIEVEMENT"
                        android:layout_marginTop="3dp"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/achievmentItem5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement5Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement5TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:layout_marginTop="3dp"
                        android:text="ACHIEVEMENT"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/achievmentItem6"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:orientation="vertical"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp">

                    <ImageView
                        android:id="@+id/achievement6Img"
                        android:layout_width="80dp"
                        android:layout_height="80dp"
                        android:layout_margin="5dp"
                        android:layout_gravity="center"
                        android:src="@drawable/blue_circle" />

                    <TextView
                        android:id="@+id/achievement6TextView"
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/fonts"
                        android:textSize="14sp"
                        android:textFontWeight="700"
                        android:layout_marginTop="3dp"
                        android:text="ACHIEVEMENT"
                        android:layout_gravity="center"
                        android:textColor="@color/colorPrimaryDark"/>

                </LinearLayout>

            </LinearLayout>

        </LinearLayout>



    </LinearLayout>

